{
    "manifest_version": 3,
    "name": "Tubescraper",
    "description": "Scrap all video data in channel and export it to csv",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "images/icon.png"
    },
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["https://www.youtube.com/*"]
  }